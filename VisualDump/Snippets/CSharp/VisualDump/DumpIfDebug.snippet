<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Dump If DEBUG</Title>
      <Shortcut>didebug</Shortcut>
      <Description>Inserts code that activates Dumping only with DEBUG</Description>
      <Author>Kir_Antipov</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet><Code Language="CSharp"><![CDATA[#if !DEBUG
    DumpExtensions.DisableDumping();
#endif$end$]]>
    </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>